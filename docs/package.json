{
  "name": "docs",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "clean:build": "shx rm -rf ./build",
    "clean:docs-packages": "shx rm -rf ./docs/packages/*",
    "clean:docs-api": "shx rm -rf ./docs/api-input/* && shx rm -rf ./docs/api/*",
    "clean": "run-p clean:build clean:docs-packages clean:docs-api",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build:api": "api-documenter markdown -i ./docs/api-input -o ./docs/api",
    "build:docs": "docusaurus build",
    "build": "run-s build:api build:docs",
    "swizzle": "docusaurus swizzle",
    "release": "gh-pages -d ./build -t",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.14",
    "@docusaurus/preset-classic": "2.0.0-beta.14",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.33",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3"
  }
}
